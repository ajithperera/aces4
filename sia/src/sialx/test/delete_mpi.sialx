sial delete_mpi
	special list_block_map
	special print_block r
	predefined int norb
	aoindex i = 1:norb
	aoindex j = 1:norb
	local a[i,j]
	local aa[i,j]
	distributed b[i,j]
	distributed c[i,j]
	
	create b
	create c
	allocate a[*,*]
	
	println "Sending blocks"	
	pardo i
		print i
		execute list_block_map
		do j
			print j
			a[i,j] = 42.0
			aa[i,j] = 55.0
			put b[i,j] = a[i,j]
			put c[i,j] += aa[i,j]
			put c[i,j] += aa[i,j]
		enddo j
	endpardo  i
	sip_barrier
	delete b
	delete c
	
endsial delete_mpi