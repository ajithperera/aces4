      PROC PRINTIIJ
#     --------------
           esum = 0.0 
           etot = 0.0 
           PARDO i, i1
              GET Iij_aa[i,i1]
              etemp = Iij_aa[i,i1]*Iij_aa[i,i1]
              esum += etemp 
           ENDPARDO i, i1
           sip_barrier 
           collective etot += esum 
           print "checksum of Iij_aa"
           print  etot 
#
           esum = 0.0 
           etot = 0.0 
           PARDO j, j1
              GET Iij_bb[j,j1]
              etemp = Iij_bb[j,j1]*Iij_bb[j,j1]
              esum += etemp 
           ENDPARDO j, j1
           sip_barrier 
           collective etot += esum 
           print "checksum of Iij_bb"
           print etot 
