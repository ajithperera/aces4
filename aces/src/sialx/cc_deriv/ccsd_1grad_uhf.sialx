      PROC PRINTIAB
#     -------------
#
           esum = 0.0 
           etot = 0.0 
           PARDO a, a1
              GET Iab_aa[a,a1]
              etemp = Iab_aa[a,a1]*Iab_aa[a,a1] 
              esum += etemp 
           ENDPARDO a, a1
           sip_barrier 
           collective etot += esum 
           print "checksum of I_a_bb"
