#
           esum = 0.0 
           etot = 0.0 
           PARDO b, b1
              GET Iab_bb[b,b1]
              etemp = Iab_bb[b,b1]*Iab_bb[b,b1] 
              esum += etemp 
           ENDPARDO b, b1
